{
    "extends": [
      "react-app",
      "eslint:recommended",
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      // "prettier/prettier": "error",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "prettier/prettier": ["error", { "singleQuote": true }]
    }
}